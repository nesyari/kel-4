#hurdle 1
def turn_right():
    turn_left()
    turn_left()
    turn_left()
    
def bypass_wall():
    move()
    turn_left()
    move()
    turn_right()
    move()
    turn_right()
    move()
    turn_left()
while not at_goal():
    bypass_wall()

#hurdle 2
def turn_right():
    turn_left()
    turn_left()
    turn_left()
    
def lompat():
    move()
    turn_left()
    move()
    turn_right()
    move()
    turn_right()
    move()
    turn_left()
while not at_goal():#diulangi selama tidak di goal
    lompat()
